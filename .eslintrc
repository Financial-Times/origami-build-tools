{
    "env": {
        "node": true,
        "es6": true
    },
    "ecmaFeatures": {
        "modules": false
    },
    "rules": {
        "eqeqeq": 2,
        "no-underscore-dangle": 0,
        "guard-for-in": 2,
        "no-extend-native": 2,
        "wrap-iife": 2,
        "new-cap": 2,
        "no-caller": 2,
        "no-irregular-whitespace": 1,
        "no-trailing-spaces": 2,
        "strict": [2, "global"],
        "quotes": [1, "single"],
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": 2,
        "one-var": [2, "never"]
    },
    "globals": {
        "fetch": true
    }
}
