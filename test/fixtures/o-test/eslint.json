{
	"parser": "babel-eslint",
	"ecmaFeatures": {
		"modules": false
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"eqeqeq": 2,
		"guard-for-in": 2,
		"no-extend-native": 2,
		"wrap-iife": 2,
		"new-cap": 2,
		"no-caller": 2,
		"no-irregular-whitespace": 1,
		"no-trailing-spaces": 2,
		"no-multri-str": 0,
		"dot-notation": 0,
		"strict": [2, "never"],
		"valid-jsdoc": 1,
		"no-loop-func": 2,
		"no-multi-spaces": 2,
		"no-multiple-empty-lines": 2,
		"one-var": [2, "never"],
		"constructor-super": 2,
		"no-this-before-super": 2,
		"prefer-const": 1
	}
}
